{
  "manifest_version": 2,
  "name": "Ataraxia - New Tab with Daily Wallpaper",
  "description": "Override the new tab page with beautiful daily wallpaper. Multiple search engines supported.",
  "version": "0.7",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "icons": {
    "16": "./icon.png",
    "32": "./icon.png",
    "48": "./icon.png",
    "128": "./icon.png"
  },
  "permissions": [
  	"*://*.bing.com/*",
    "topSites",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
